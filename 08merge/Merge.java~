public class Merge{
    public static void main (String[] args){
	int[] a = new int[5];
	mergesort(a);
	for (int i = 0; i < a.size; i++){
	    System.out.print(a[i]);
	}
    }
    public mergesort(int[] a){
	if (a.size == 1){
	    return a;
	}
	else{
	    int[] left = new int[a.size/2];
	    int[] right = new int[a.size-left.size];
	    for (int i = 0; i < left.size; i++){left[i] = a[i];}
	    for (int i = 0; i < right.size; i++){right[i] = a[i+left.size];
	}
    }
}
